//RXPCIVP2 JOB (1),'RXPCIVP$',CLASS=A,COND=(5,LT)
//*
//* RXPATTRN - REXX string pattern check
//* Compare performance LPALIB to STEPLIB
//*
//*
//* 1 - Running from LPA
//*
//LPA1     EXEC PGM=IKJEFT1B,PARM='%RXPCIVP2 10   *who*'
//SYSEXEC  DD DISP=SHR,DSN=CBT669.PP.RXPATTRN.V001.LIB
//TESTDATA DD DISP=SHR,DSN=WJ.LIB.DATA(DRWHO)
//SYSTSPRT DD SYSOUT=*,OUTLIM=(5000)
//SYSTSIN  DD DUMMY
//*
//LPA2     EXEC PGM=IKJEFT1B,PARM='%RXPCIVP2 100  *who*'
//SYSEXEC  DD DISP=SHR,DSN=CBT669.PP.RXPATTRN.V001.LIB
//TESTDATA DD DISP=SHR,DSN=WJ.LIB.DATA(DRWHO)
//SYSTSPRT DD SYSOUT=*,OUTLIM=(5000)
//SYSTSIN  DD DUMMY
//*
//* 2 - Running from steplib
//*
//SLB1     EXEC PGM=IKJEFT1B,PARM='%RXPCIVP2 10   *who*'
//STEPLIB  DD DISP=SHR,DSN=CBT669.LINKLIB
//SYSEXEC  DD DISP=SHR,DSN=CBT669.PP.RXPATTRN.V001.LIB
//TESTDATA DD DISP=SHR,DSN=WJ.LIB.DATA(DRWHO)
//SYSTSPRT DD SYSOUT=*,OUTLIM=(5000)
//SYSTSIN  DD DUMMY
//*
//SLB2     EXEC PGM=IKJEFT1B,PARM='%RXPCIVP2 100  *who*'
//STEPLIB  DD DISP=SHR,DSN=CBT669.LINKLIB
//SYSEXEC  DD DISP=SHR,DSN=CBT669.PP.RXPATTRN.V001.LIB
//TESTDATA DD DISP=SHR,DSN=WJ.LIB.DATA(DRWHO)
//SYSTSPRT DD SYSOUT=*,OUTLIM=(5000)
//SYSTSIN  DD DUMMY
